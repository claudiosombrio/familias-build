<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="br.com.celk.familias" version="4">
  <name>Fam√≠lias</name>
  <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
  <content src="index.html"/>
  <access origin="*"/>
  <icon src="resources/android/icon/drawable-xhdpi-icon.png"/>
  <preference name="SplashScreen" value="screen"/>
  <preference name="SplashScreenDelay" value="30000"/>
  <preference name='phonegap-version' value='cli-5.1.1' />	

  <gap:plugin name="com.brodysoft.sqliteplugin" version="1.0.4" />
  <gap:plugin name="com.plugin.datepicker" version="0.4.0" />
  <gap:plugin name="org.apache.cordova.camera" version="0.3.2" />
  <gap:plugin name="io.github.pwlin.cordova.plugins.fileopener2" version="1.0.11" />
  <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
  <gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />
  <!--gap:plugin name="org.apache.cordova.file-transfer" version="0.4.6" /-->
  <gap:plugin name="org.apache.cordova.file" version="1.3.1" />
  <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
  <gap:plugin name="org.apache.cordova.network-information" version="0.2.12" />
  <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />
  <gap:plugin name="uk.co.whiteoctober.cordova.appversion" version="0.1.4" />

  <gap:platform name="ios" />
  <gap:platform name="android" />
  <gap:platform name="winphone" />
</widget>
